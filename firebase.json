{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/articles/**",
        "destination": "/articles/articles.html"
      },
      {
        "source": "/inkers/**",
        "destination": "/inkers/inkers.html"
      }
    ]
  }
}
